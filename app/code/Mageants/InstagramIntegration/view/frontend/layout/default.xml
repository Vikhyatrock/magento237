<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Html\Link" name="instagram.link" after="register-link" ifconfig="instagramintegration/instagram_integration/enable_frontend">
                <arguments>
                    <argument name="label" xsi:type="string" translate="false">Instagram</argument>
                    <argument name="path" xsi:type="string" translate="false">instagramintegration/gallery/instagramlist</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>